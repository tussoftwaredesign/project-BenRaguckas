<?xml version="1.0"?>
<apilist xmlns="http://www.test.org"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.test.org https://raw.githubusercontent.com/tussoftwaredesign/project-BenRaguckas/go-rest-api/config/config.xsd">
    <default-entrypoints>
        <default-get>default/get/:a/:b</default-get>
        <default-post>default/post</default-post>
        <default-put>default/put/:a</default-put>
    </default-entrypoints>
    <endpoint>
        <uri use_params="true" use_body="false" path_params="bucket item">/custom/:bucket/:item</uri>
        <action method="GET"><function-byname>getItem</function-byname></action>
        <action method="PUT"><function-byname>putItemNamed</function-byname></action>
    </endpoint>
    <endpoint>
        <uri use_params="true" use_body="false" path_params="bucket">/custom/:bucket</uri>
        <action method="GET"><function-byname>listItems</function-byname></action>
        <action method="PUT"><function-byname>putItem</function-byname></action>
    </endpoint>
    <endpoint>
        <uri  use_params="false" use_body="true" body_items="file">/custom</uri>
        <action method="POST"><function-byname>pushItem</function-byname></action>
    </endpoint>
    <endpoint>
        <uri use_params="false" use_body="true" body_items="file">/TEST</uri>
        <action method="POST"><function-byname>predefinedBackground</function-byname></action>
    </endpoint>
    <endpoint>
        <uri use_params="false" use_body="true" body_items="file">/gray</uri>
        <action method="POST"><function-byname>predefinedGray</function-byname></action>
    </endpoint>
</apilist>